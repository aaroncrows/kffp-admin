{
  "name": "FreeformWebApplication",
  "version": "0.0.1",
  "description": "The admin, playlist manager, and API for Freeform Portland ",
  "main": "index.js",
  "scripts": {
    "dev": "npm run watch:css & npm run dev:js",
    "dev:js": "nodemon -e js,hbs -w src --ignore src/client",
    "build:js": "NODE_ENV=production webpack",
    "build:css": "npm run build:sass && npm run build:postcss",
    "watch:css": "node-sass src/client/css -o public -w & postcss -c ./postcss.json -w",
    "start": "NODE_ENV=production node index.js",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build:sass": "node-sass src/client/css -o public",
    "build:postcss": "postcss -c ./postcss.json"
  },
  "repository": {
    "url": "git+https://github.com/marciaga/playlist-v2.git",
    "type": "git"
  },
  "keywords": [
    "hapi",
    "react",
    "redux"
  ],
  "author": "Arciaga, other folks tbd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marciaga/playlist-v2/issues"
  },
  "homepage": "https://github.com/marciaga/playlist-v2#readme",
